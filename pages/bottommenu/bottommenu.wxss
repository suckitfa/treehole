.bottomrow {
  background-color: white;
  width: 100%;
  height: 9%;
  bottom: 0rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  position: fixed;
}

.bottomtab {
  width: 250rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.bottomImg {
  margin-top: 10rpx;
  width: 130rpx;
  height: 110rpx;
  border-radius: 10rpx;
}

.bottomtabText {
  font-size: 35rpx;
  color: #979797;
}